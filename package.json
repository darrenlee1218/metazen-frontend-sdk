    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "jest": "^28.1.3",
    "lint-staged": "~12.1.5",
    "postcss": "~8.4.14",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.3",
