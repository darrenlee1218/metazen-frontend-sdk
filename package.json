  "scripts": {
    "start": "HOST=${HOST:-localhost} PORT=${PORT:-3000} react-app-rewired --max_old_space_size=4096 start",
    "start:ci": "PORT=3000 react-app-rewired  --max_old_space_size=4096  start",
    "start:dev-in-docker": "PORT=80 react-app-rewired start",
    "start:mock": "prism mock -p ${PORT:-3001} ./local_mock_server/swagger.yaml",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "image:local": "react-app-rewired build && dev-tools image -r localhost.example:5000 -n frontend -e local -f Dockerfile.local",
    "test": "env-cmd -f .env.test react-app-rewired test",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src/* --fix --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern",
    "prepare": "is-ci || husky install",
    "test:ci": "npm test -- --collectCoverage --ci --reporters=default  --watchAll=false",
    "dev": "concurrently \"npm run start\" \"npm run  start:mock \"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "audit:prod": "npm audit --omit=dev"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.18.6",
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@stoplight/prism-cli": "^4.10.5",
